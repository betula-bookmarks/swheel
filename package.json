{
  "name": "swheel",
  "version": "1.1.0",
  "main": "dist/cjs/index.js",
  "module": "dist/es/index.mjs",
  "unpkg": "dist/es/index.mjs",
  "svelte": "./src/Router/index.js",
  "license": "MIT",
  "author": "Dmitry Kutran <kutran.dmitry@gmail.com> (https://github.com/qutran)",
